{
	"name": "discorss",
	"description": "Manage Discord RSS feeds using webhooks",
	"version": "0.11.0",
	"scripts": {
		"sync": "svelte-kit sync",
		"dev": "vite dev --https",
		"build": "vite build",
		"start": "node build",
		"build:runner": "tsc --module es2022 --target es2022 --moduleResolution node --allowSyntheticDefaultImports true --outDir runner src/runner.ts",
		"runner": "node runner/runner.js"
	},
	"type": "module",
	"dependencies": {
		"node-fetch": "^2.6.1",
		"redis": "^4.2.0",
		"rss-parser": "^3.10.0",
		"sk-auth": "^0.4.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-node": "^1.0.0-next.78",
		"@sveltejs/kit": "^1.0.0-next.360",
		"@types/node-fetch": "^2.6.2",
		"carbon-components-svelte": "^0.67.0",
		"carbon-icons-svelte": "^10.23.0",
		"carbon-preprocess-svelte": "^0.9.1",
		"svelte": "^3.17.3",
		"svelte-preprocess": "^4.3.0",
		"tslib": "^2.0.1",
		"typescript": "^4.0.3"
	}
}
